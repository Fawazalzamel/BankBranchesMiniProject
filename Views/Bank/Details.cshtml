    <thead>
    <td>ID</td>
    <td>Branch Manger Name</td>
    <td>Branch Location Name</td>
    <td>Location URL</td>
    <td>Amount of Employees in Branch</td>
    </thead>

    <tbody>
        
        
            <tr>

                <td>@Model.Id</td>
